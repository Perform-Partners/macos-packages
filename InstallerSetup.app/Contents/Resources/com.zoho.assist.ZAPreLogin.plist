<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.zoho.assist.ZAPreLogin</string>
        <key>LimitLoadToSessionType</key>
        <string>LoginWindow</string>
        <key>RunAtLoad</key>
        <true/>
        <key>WorkingDirectory</key>
        <string>/Library/Application Support/ZohoAssist</string>
        <key>ProgramArguments</key>
        <array>
            <string>/Library/Application Support/ZohoAssist/ZAPreLogin</string>
            <string>service</string>
            <string>zoho</string>
        </array>
        <key>KeepAlive</key>
        <true/>
    </dict>
</plist>
